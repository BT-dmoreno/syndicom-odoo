<odoo>
    <template id="email_designer_snippets_inherit" inherit_id="mass_mailing.email_designer_snippets">
        <xpath expr="//div[@id='email_designer_default_headers']" position="before">
            <div id="syndicom_new_designer_default" class="o_panel">
                <div class="o_panel_header">syndicom</div>
                <div class="o_panel_body" id="syndicom_new_designer_default">

                    <t t-snippet="syndicom_mass_mailing_themes.syndicom_new_buttons_blue" t-thumbnail="/syndicom_mass_mailing_themes/static/src/img/snippets_thumbs/syndicom_call_to_action.svg"/>
                    <t t-snippet="syndicom_mass_mailing_themes.syndicom_new_buttons_black" t-thumbnail="/syndicom_mass_mailing_themes/static/src/img/snippets_thumbs/syndicom_call_to_action.svg"/>
                    <t t-snippet="syndicom_mass_mailing_themes.syndicom_new_buttons_yellow" t-thumbnail="/syndicom_mass_mailing_themes/static/src/img/snippets_thumbs/syndicom_call_to_action.svg"/>
                
                </div>
            </div>
        </xpath>
    </template>
</odoo>
