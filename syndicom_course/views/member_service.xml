<odoo>

    <record id="view_member_service_form" model="ir.ui.view">
        <field name="name">member.service.form.inherit</field>
        <field name="model">member.service</field>
        <field name="inherit_id" ref="syndicom_contacts.view_member_service_form"/>
        <field name="arch" type="xml">
           
             <xpath expr="//field[@name='other_partner_id']" position="after">
                <field name="course_id" />
            </xpath>

        </field>
    </record>
    <record id="view_member_service_tree" model="ir.ui.view">
        <field name="name">member.service.tree.inherit</field>
        <field name="model">member.service</field>
        <field name="inherit_id" ref="syndicom_contacts.view_member_service_tree"/>
        <field name="arch" type="xml">
           
             <xpath expr="//field[@name='other_partner_id']" position="after">
                <field name="course_id"  />
           
            </xpath>
            
        </field>
    </record>

</odoo>
